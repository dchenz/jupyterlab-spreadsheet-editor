{
  "name": "spreadsheet-editor",
  "version": "0.2.0",
  "description": "JupyterLab spreadsheet (csv/tsv) editor",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "csv",
    "tsv",
    "spreadsheeet",
    "excel"
  ],
  "homepage": "https://github.com/krassowski/jupyterlab-spreadsheet-editor",
  "bugs": {
    "url": "https://github.com/krassowski/jupyterlab-spreadsheet-editor/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Micha≈Ç Krassowski",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/icons/**/*.{svg}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/krassowski/jupyterlab-spreadsheet-editor.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.1.2",
    "@jupyterlab/apputils": "^2.1.1",
    "@jupyterlab/coreutils": "^4.1.0",
    "@jupyterlab/docregistry": "^2.1.2",
    "@jupyterlab/documentsearch": "^2.1.2",
    "@jupyterlab/filebrowser": "^2.1.2",
    "@jupyterlab/fileeditor": "^2.1.2",
    "@jupyterlab/launcher": "^2.1.1",
    "@jupyterlab/mainmenu": "^2.1.1",
    "@jupyterlab/ui-components": "^2.1.1",
    "@lumino/commands": "^1.11.0",
    "@lumino/coreutils": "^1.5.0",
    "@lumino/messaging": "^1.4.0",
    "@lumino/signaling": "^1.4.0",
    "@lumino/widgets": "^1.13.0",
    "@types/codemirror": "0.0.76",
    "@types/jexcel": "^3.9.1",
    "@types/papaparse": "^5.0.3",
    "jexcel": "^4.2.0",
    "papaparse": "^5.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "1.16.4",
    "rimraf": "^2.6.1",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
